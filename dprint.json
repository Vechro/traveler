{
  "incremental": true,
  "lineWidth": 120,
  "useTabs": false,
  "indentWidth": 2,
  "json": {},
  "markdown": {},
  "toml": {},
  "typescript": {
    "nextControlFlowPosition": "sameLine"
  },
  "includes": ["**/*.{ts,js,cjs,mjs,json,md,toml}"],
  "excludes": ["**/node_modules", "**/*-lock.json", "**/.next", "**/.turbo", "**/dist", "pnpm-lock.yaml", "**/target"],
  "plugins": [
    "https://plugins.dprint.dev/json-0.16.0.wasm",
    "https://plugins.dprint.dev/markdown-0.14.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/typescript-0.77.0.wasm"
  ]
}
