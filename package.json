{
  "name": "traveler",
  "private": true,
  "scripts": {
    "build": "turbo run build --parallel",
    "dev": "turbo run dev --parallel --no-cache",
    "compose:up": "docker compose up",
    "compose:destroy": "docker compose down -v --remove-orphans",
    "format": "pnpm exec dprint fmt",
    "lint": "turbo run lint",
    "globe": "pnpm --filter=./apps/globe",
    "globe:dev": "turbo run dev --scope=globe",
    "orbit": "pnpm --filter=./apps/orbit",
    "orbit:dev": "turbo run dev --scope=orbit",
    "turtle": "pnpm --filter=./packages/turtle",
    "turtle:dev": "turbo run dev --scope=turtle"
  },
  "devDependencies": {
    "dprint": "^0.32.2",
    "eslint": "^8.26.0",
    "@vechro/eslint-config-traveler": "workspace:*",
    "turbo": "^1.6.3",
    "typescript": "^4.8.4"
  },
  "packageManager": "pnpm@7.14.2"
}
